{
  "name": "ia-image-generator",
  "version": "1.0.0",
  "description": "AI Image Generator - Full Stack Application",
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "dev:backend": "npm --prefix backend run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "install": "npm-run-all install:backend install:frontend",
    "install:backend": "npm --prefix backend install",
    "install:frontend": "npm --prefix frontend install",
    "build": "npm-run-all build:backend build:frontend",
    "build:backend": "npm --prefix backend run build",
    "build:frontend": "npm --prefix frontend run build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "cors": "^2.8.5"
  }
}
